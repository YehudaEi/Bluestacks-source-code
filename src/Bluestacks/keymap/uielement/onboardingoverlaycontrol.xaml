<UserControl x:Class="BlueStacks.BlueStacksUI.OnboardingOverlayControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:Common="clr-namespace:BlueStacks.Common;assembly=HD-Common"
             xmlns:local="clr-namespace:BlueStacks.BlueStacksUI"
             Background="White">
  <Border x:Name="mBorder" BorderThickness="0" RenderOptions.EdgeMode="Unspecified"
          SnapsToDevicePixels="false" BorderBrush="Black" Opacity="1" CornerRadius="4">
    <Grid>
      <Grid x:Name="mGrid">
        <Common:CustomPictureBox x:Name="mOnboardingImg" Visibility="Collapsed" ImageName="onboarding_step1"/>
      </Grid>
    </Grid>
  </Border>
</UserControl>